{
  "name": "policy-MongoDB-Financtial",
  "sparkStreamingWindow": 6000,
  "rawData": {
    "enabled": "false",
    "partitionFormat": "day",
    "path": "myTestParquetPath"
  },
  "checkpointing": {
    "path": "checkpoint",
    "timeDimension": "minute",
    "granularity": "minute",
    "interval": 30000,
    "timeAvailability": 60000
  },
  "inputs": [
    {
      "name": "in-flume",
      "type": "Flume",
      "configuration": {
        "type": "pull",
        "addresses": "localhost:10999",
        "storageLevel": "MEMORY_AND_DISK_SER_2",
        "maxBatchSize": 500
      }
    }
  ],
  "transformations": [
    {
      "name": "morphline-parser",
      "order": 0,
      "type": "Morphlines",
      "configuration": {
        "morphline": {
          "id": "morphline1",
          "importCommands": [
            "org.kitesdk.**",
            "com.stratio.ingestion.morphline.**"
          ],
          "commands": [
            {
              "readJson": {

              }
            },
            {
              "extractJsonPaths": {
                "paths": {
                  "appName": "/appName",
                  "method": "/method",
                  "date": "/date",
                  "appCountry": "/appCountry",
                  "appPlatform": "/appPlatform",
                  "appVersion": "/appVersion",
                  "uid": "/uid",
                  "device": "/device",
                  "latitude": "/latitude",
                  "longitude": "/longitude",
                  "osVersion": "/osVersion",
                  "lang": "/lang",
                  "appLang": "/appLang",
                  "user_id": "/user_id",
                  "connection": "/connection",
                  "timestamp": "/timestamp",
                  "session": "/session",
                  "extra1": "/extra1",
                  "extra2": "/extra2",
                  "extra3": "/extra3",
                  "source": "/source",
                  "environment": "/environment",
                  "platform": "/platform"
                }
              }
            },
            {
              "addValues": {
                "geo": "@{latitude}__@{longitude}"
              }
            },
            {
              "removeFields": {
                "blacklist": [
                  "literal:_attachment_body",
                  "literal:message"
                ]
              }
            }
          ]
        }
      }
    },
    {
      "name": "datetime",
      "order": 1,
      "inputField": "timestamp",
      "outputFields": [
        "timestamp"
      ],
      "type": "DateTime",
      "configuration": {
        "timestamp": "unix"
      }
    }
  ],
  "cubes": [
    {
      "name": "extra1",
      "dimensions": [
        {
          "name": "extra1",
          "field": "extra1"
        },
        {
          "field": "timestamp",
          "precision": "minute",
          "type": "DateTime",
          "name": "minute"
        }
      ],
      "operators": [
        {
          "name": "countoperator",
          "type": "Count",
          "configuration": {

          }
        }
      ]
    },
    {
      "name": "platform_device",
      "dimensions": [
        {
          "name": "platform",
          "field": "platform"
        },
        {
          "name": "device",
          "field": "device"
        },
        {
          "dimension": "timestamp",
          "precision": "minute",
          "name": "minute"
        }
      ],
      "operators": [
        {
          "name": "countoperator",
          "type": "Count",
          "configuration": {

          }
        }
      ]
    },
    {
      "name": "appname",
      "dimensions": [
        {
          "field": "appName",
          "name": "appName"
        }
      ],
      "operators": [
        {
          "name": "countoperator",
          "type": "Count",
          "configuration": {

          }
        }
      ]
    },
    {
      "name": "session",
      "dimensions": [
        {
          "name": "session",
          "field": "session"
        },
        {
          "dimension": "timestamp",
          "precision": "minute",
          "type": "DateTime",
          "name": "minute"
        }
      ],
      "operators": [
        {
          "name": "countoperator",
          "type": "Count",
          "configuration": {

          }
        }
      ]
    },
    {
      "name": "applang",
      "dimensions": [
        {
          "name": "appLang",
          "field": "appLang"
        },
        {
          "name": "timestamp",
          "type": "DateTime",
          "field": "timestamp",
          "precision": "minute"
        }
      ],
      "operators": [
        {
          "name": "countoperator",
          "type": "Count",
          "configuration": {

          }
        }
      ]
    },
    {
      "name": "geo",
      "dimensions": [
        {
          "field": "geo",
          "type": "GeoHash",
          "precision": "precision6",
          "name": "precision6"
        }
      ],
      "operators": [
        {
          "name": "countoperator",
          "type": "Count",
          "configuration": {

          }
        }
      ]
    },
    {
      "name": "appCountry",
      "dimensions": [
        {
          "name": "appCountry",
          "field": "appCountry"
        },
        {
          "dimension": "timestamp",
          "precision": "minute",
          "type": "DateTime",
          "name": "minute"
        }
      ],
      "operators": [
        {
          "name": "countoperator",
          "type": "Count",
          "configuration": {

          }
        }
