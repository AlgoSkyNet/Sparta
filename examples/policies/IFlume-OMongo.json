          "name": "minute"
        }
      ],
      "operators": [
        {
          "name": "countoperator",
          "type": "Count",
          "configuration": {

          }
        }
      ]
    },
    {
      "name": "appPlatform_time",
      "dimensions": [
        {
          "name": "appPlatform",
          "field": "appPlatform"
        }
      ],
      "operators": [
        {
          "name": "countoperator",
          "type": "Count",
          "configuration": {

          }
        }
      ]
    },
    {
      "name": "appCountry",
      "dimensions": [
        {
          "name": "appCountry",
          "field": "appCountry"
        }
      ],
      "operators": [
        {
          "name": "countoperator",
          "type": "Count",
          "configuration": {

          }
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "out-mongo",
      "type": "MongoDb",
      "configuration": {
        "clientUri": "mongodb://localhost:27017",
        "dbName": "sparkta"

      }
    }
  ]
}