{
  "name": "policy-flume",
  "sparkStreamingWindow": 6000,
  "rawData": {
    "enabled": "false",
    "partitionFormat": "day",
    "path": "myTestParquetPath"
  },
  "checkpointing": {
    "path": "checkpoint",
    "timeDimension": "minute",
    "granularity": "minute",
    "interval": 30000,
    "timeAvailability": 60000
  },
  "inputs": [
    {
      "name": "in-flume",
      "type": "Flume",
      "configuration": {
        "type": "pull",
        "addresses": "localhost:10999",
        "storageLevel": "MEMORY_AND_DISK_SER_2"
      }
    }
  ],
  "transormations": [
    {
      "name": "morphline",
      "order": 0,
      "outputFields": [
        "message"
      ],
      "type": "Morphlines",
      "configuration": {
        "morphline": {
          "id": "test1",
          "importCommands": [
            "org.kitesdk.**"
          ],
          "commands": [
            {
              "readLine": {
                "charset": "UTF-8"
              }
            }
          ]
        }
      }
    }
  ],
  "cubes": [
    {
      "name": "messages",
      "dimensions": [
        {
          "field": "message",
          "name": "message"
        }
      ],
      "operators": [
        {
          "measureName": "count-operator",
          "type": "Count",
          "configuration": {}
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "out-print",
      "type": "Print",
      "configuration": {
        "multiplexer": "true",
        "isAutoCalculateId": "true"
      }
    }
  ]
}