{
  "name": "policy-flume",
  "description": "policy-flume",
  "sparkStreamingWindow": 6000,
  "checkpointPath": "checkpoint",
  "rawData": {
    "enabled": "false",
    "partitionFormat": "day",
    "path": "myTestParquetPath"
  },
  "input":
    {
      "name": "in-flume",
      "type": "Flume",
      "configuration": {
        "type": "pull",
        "addresses": "localhost:10999"
      }
    }
  ,
  "transormations": [
    {
      "name": "morphline",
      "order": 0,
      "outputFields": [
        "message"
      ],
      "type": "Morphlines",
      "configuration": {
        "morphline": {
          "id": "test1",
          "importCommands": [
            "org.kitesdk.**"
          ],
          "commands": [
            {
              "readLine": {
                "charset": "UTF-8"
              }
            }
          ]
        }
      }
    }
  ],
  "cubes": [
    {
      "name": "messages",
      "checkpointConfig": {
        "timeDimension": "minute",
        "granularity": "minute",
        "interval": 30000,
        "timeAvailability": 60000
      },
      "dimensions": [
        {
          "field": "message",
          "name": "message"
        }
      ],
      "operators": [
        {
          "name": "countoperator",
          "type": "Count",
          "configuration": {}
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "out-print",
      "type": "Print",
      "configuration": {
        "multiplexer": "true",
        "isAutoCalculateId": "true"
      }
    }
  ]
}