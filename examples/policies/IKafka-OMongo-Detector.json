{
  "name": "policy-kafka",
  "description": "policy-kafka",
  "sparkStreamingWindow": 2000,
  "checkpointPath": "checkpoint",
  "rawData": {
    "enabled": "true",
    "partitionFormat": "day",
    "path": "myTestParquetPath"
  },
  "input":
    {
      "name": "in-kafka",
      "type": "Kafka",
      "configuration": {
        "topics": [{"topic": "detector", "partition" : "1"},{"topic": "detector4", "partition" : "1"}],
        "zookeeper.connect": [{"host": "localhost", "port": "2181"}],
        "kafkaParams.group.id": "testgroup"
      }
    }
  ,
  "transformations": [
    {
      "name": "detector-parser",
      "order": 1,
      "type": "Detector",
      "outputFields": [
        "company_root",
        "ou_vehicle",
        "recorded_at_ms",
        "geo",
        "rpm_event_avg",
        "asset",
        "odometer",
        "odometerNum",
        "rpmAvgNum",
        "path_id"
      ],
      "configuration": {}
    },
    {
      "name": "recorded_at_ms-parser",
      "order": 2,
      "type": "DateTime",
      "inputField": "recorded_at_ms",
      "outputFields": [
        "recorded_at_ms"
      ],
      "configuration": {
        "recorded_at_ms": "unixMillis"
      }
    }
  ],
  "cubes": [
    {
      "name": "testCube1",
      "checkpointConfig": {
        "timeDimension": "minute",
        "granularity": "minute",
        "interval": 30000,
        "timeAvailability": 60000
      },
      "dimensions": [
        {
          "name": "company_root",
          "field": "company_root"
        },
        {
          "name": "ou_vehicle",
          "field": "ou_vehicle"
        },
        {
          "name": "precision12",
          "field": "geo",
          "type": "GeoHash",
          "precision": "precision12"
        },
        {
          "name": "minute",
          "field": "recorded_at_ms",
          "type": "DateTime",
          "precision": "minute"
        }
      ],
      "operators": [
        {
          "name": "avgoperator",
          "type": "Avg",
          "configuration": {
            "inputField": "rpmAvgNum"
          }
        }
      ]
    },
    {
      "name": "testCube2",
      "checkpointConfig": {
        "timeDimension": "minute",
        "granularity": "minute",
        "interval": 30000,
        "timeAvailability": 60000
      },
      "dimensions": [
        {
          "name": "company_root",
          "field": "company_root"
        },
        {
          "name": "ou_vehicle",
          "field": "ou_vehicle"
        },
        {
          "name": "asset",
          "field": "asset"
        },
        {
          "name": "recorded_at_ms",
          "field": "recorded_at_ms",
          "type": "DateTime",
          "precision": "minute"
        },
        {
          "name": "path_id",
          "field": "path_id"
        }
      ],
      "operators": [
        {
          "name": "maxoperator",
          "type": "Max",
          "configuration": {
            "inputField": "odometerNum"
          }
        },
        {
          "name": "minoperator",
          "type": "Min",
          "configuration": {
            "inputField": "odometerNum"
          }
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "out-mongo",
      "type": "MongoDb",
      "configuration": {
        "hosts": [{"host": "localhost" , "port": "27017" }],
        "dbName": "sparkta"
      }
    }
  ]
}