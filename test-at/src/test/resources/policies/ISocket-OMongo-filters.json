{

  "name": "ATSocketMongoFilters",
  "description": "No description",
  "sparkStreamingWindow": 2000,
  "checkpointPath": "checkpoint",
  "rawData": {
    "enabled": "false",
    "partitionFormat": "day",
    "path": "myTestParquetPath"
  },
  "input":
  {
    "name": "in-socket",
    "type": "Socket",
    "configuration": {
      "hostname": "localhost",
      "port": "10666"
    }
  }
,
  "transformations": [
    {
      "name": "morphline-parser",
      "order": 0,
      "type": "Morphlines",
      "outputFields": [
        "idTweet",
        "text",
        "userMention",
        "hashtag",
        "url",
        "media",
        "currentUserRetweetId",
        "user",
        "locationFirstLevel",
        "locationSecondLevel",
        "followersCount",
        "retweetCount",
        "favorited",
        "unique"
      ],
      "configuration": {
        "morphline": {
          "id": "morphline1",
          "importCommands": [
            "org.kitesdk.**"
          ],
          "commands": [
            {
              "readCSV": {
                "separator": ",",
                "columns": [
                  "idTweet",
                  "text",
                  "userMention",
                  "hashtag",
                  "url",
                  "media",
                  "currentUserRetweetId",
                  "user",
                  "locationFirstLevel",
                  "locationSecondLevel",
                  "followersCount",
                  "retweetCount",
                  "favorited",
                  "unique"
                ],
                "trim": "true",
                "charset": "UTF-8",
                "quoteChar": "\"",
                "commentPrefix": "#"
              }
            }
          ]
        }
      }
    },
    {
      "name": "timestamp",
      "order": 1,
      "type": "DateTime",
      "outputFields": [
        "time"
      ],
      "configuration": {
        "inputFormat": "autoGenerated"
      }
    }
  ],
  "cubes": [
    {
      "name": "favorited",
      "dimensions": [
        {
          "name": "idTweet",
          "field": "idTweet",
          "precision": "identity",
          "type": "Default"
        },
        {
          "name": "text",
          "field": "text",
          "precision": "identity",
          "type": "Default"
        },
        {
          "name": "location",
          "field": "locationFirstLevel",
          "precision": "identity",
          "type": "Default"
        },
        {
          "name": "province",
          "field": "locationSecondLevel",
          "precision": "identity",
          "type": "Default"
        }
      ],
      "operators": [
        {
          "name": "sum1",
          "configuration": {
            "inputField": "favorited",
            "filters": [
              {
                "field": "unique",
                "type": "=",
                "value": "true",
                "fieldType": "string"
              }
            ]
          },
          "type": "Sum"
        }
      ],
      "checkpointConfig": {
        "timeDimension": "time",
        "interval": 300000,
        "timeAvailability": 90000,
        "granularity": "minute"
      }
    },
    {
      "name": "basic-hashtag",
      "dimensions": [
        {
          "name": "location",
          "field": "locationFirstLevel",
          "precision": "identity",
          "type": "Default"
        },
        {
          "name": "province",
          "field": "locationSecondLevel",
          "precision": "identity",
          "type": "Default"
        },
        {
          "name": "hashtag",
          "field": "hashtag",
          "precision": "identity",
          "type": "Default"
        }
      ],
      "operators": [
        {
          "name": "count1",
          "configuration": {
            "filters": [
              {
                "field": "hashtag",
                "type": "!=",
                "value": " ",
                "fieldType": "string"
              },
              {
                "field": "unique",
                "type": "=",
                "value": "false",
                "fieldType": "string"
              }
            ]
          },
          "type": "Count"
        }
      ],
      "checkpointConfig": {
        "timeDimension": "time",
        "granularity": "minute",
        "interval": 300000,
        "timeAvailability": 90000
      }
    }
  ],
  "outputs": [
    {
      "name": "out-mongo",
      "type": "MongoDb",
      "configuration": {
        "hosts": [{"host": "localhost" , "port": "60000" }],
        "dbName": "csvtest"
      }
    }
  ]
}