{
  "name": "ATSocketMongoDetector",
  "sparkStreamingWindow": 2000,
  "rawData": {
    "enabled": "false",
    "partitionFormat": "day",
    "path": "myTestParquetPath"
  },

  "checkpointing": {
    "path": "checkpoint",
    "timeDimension": "minute",
    "granularity": "minute",
    "interval": 30000,
    "timeAvailability": 6000000000

  },
  "inputs":  [
    {
      "name": "in-socket",
      "type": "Socket",
      "configuration": {
        "hostname": "localhost",
        "port": "10666"
      }
    }
  ],
  "cubes": [
    {
      "name": "rpmAvg",
      "dimensions": [
        {
          "name": "company_root",
          "field": "company_root"
        },
        {
          "name": "ou_vehicle",
          "field": "ou_vehicle"
        },
        {
          "name": "geo",
          "field": "geo",
          "type": "GeoHash",
          "precision": "precision3"
        },
        {
          "name": "recorded_at_ms",
          "field": "recorded_at_ms",
          "type": "DateTime",
          "precision": "minute"
        }
      ],
      "operators": [
        {
          "measureName": "avg-operator",
          "type": "Avg",
          "configuration": {
            "inputField": "rpm_event_avg"
          }
        }
      ]
    },
    {
      "name": "maxMinOdometer",
      "dimensions": [
        {
          "name": "company_root",
          "field": "company_root"
        },
        {
          "name": "ou_vehicle",
          "field": "ou_vehicle"
        },
        {
          "name": "asset",
          "field": "asset"
        },
        {
          "name": "recorded_at_ms",
          "field": "recorded_at_ms",
          "type": "DateTime",
          "precision": "minute"
        },
        {
          "name": "path_id",
          "field": "path_id"
        }
      ],

      "operators": [ {
        "measureName": "max-operator",
        "type": "Max",
        "configuration": {
          "inputField": "odometer"
        }
      },
        {
          "measureName": "min-operator",
          "type": "Min",
          "configuration": {
            "inputField": "odometer"
          }
        }
      ]
    }
  ],

  "outputs": [
    {
      "name": "out-mongo",
      "type": "MongoDb",
      "configuration": {
        "clientUri": "mongodb://localhost:60000",
        "dbName": "csvtest",
        "multiplexer": "false"
      }
    }
  ],
  "transformations": [
    {
      "name": "detector-parser",
      "order": 1,
      "type": "Detector",
      "configuration": {
      }
    },
    {
      "name": "recorded_at_ms-parser",
      "inputField": "recorded_at_ms",
      "outputFields": ["recorded_at_ms"],
      "order": 2,
      "type": "DateTime",
      "configuration": {
        "recorded_at_ms": "unixMillis"
      }
    }
  ]
}
